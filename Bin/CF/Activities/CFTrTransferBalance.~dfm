  inherited quMain: TADOQuery
    OnNewRecord = quMainNewRecord
    SQL.Strings = (
      'SELECT *,'
      
        'CASE WHEN FgCetak='#39'Y'#39' THEN '#39'Sudah Cetak'#39' ELSE '#39'Belum Cetak'#39' END ' +
        'as Status'
      'FROM CFTrTransferBalance')
