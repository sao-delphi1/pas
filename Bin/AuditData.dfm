  object quTransMoney: TADOQuery
    Connection = dmMain.dbConn
    BeforePost = quMainBeforePost
    Parameters = <>
    SQL.Strings = (
      'SELECT'
      
        '(SELECT COUNT(B.VoucherID) FROM CFTrKKBBDt B INNER JOIN CFTrKKBB' +
        'Hd A ON A.VoucherID=B.VoucherID WHERE A.FlagKKBB='#39'KM'#39') as KasM,'
      
        '(SELECT COUNT(B.VoucherID) FROM CFTrKKBBDt B INNER JOIN CFTrKKBB' +
        'Hd A ON A.VoucherID=B.VoucherID WHERE A.FlagKKBB='#39'KK'#39') as KasK,'
      
        '(SELECT COUNT(B.VoucherID) FROM CFTrKKBBDt B INNER JOIN CFTrKKBB' +
        'Hd A ON A.VoucherID=B.VoucherID WHERE A.FlagKKBB='#39'BM'#39') as BankM,'
      
        '(SELECT COUNT(B.VoucherID) FROM CFTrKKBBDt B INNER JOIN CFTrKKBB' +
        'Hd A ON A.VoucherID=B.VoucherID WHERE A.FlagKKBB='#39'BK'#39') as BankK,'
      '(SELECT COUNT(PaymentID) FROM APTrPaymentDt) as PembHutang,'
      
        '(SELECT COUNT(A.VoucherID) FROM CFTrKKBBDt A INNER JOIN CFTrKKBB' +
        'Hd B ON A.VoucherID=B.VoucherID WHERE B.FlagKKBB IN ('#39'APK'#39','#39'APB'#39 +
        ','#39'APC'#39')) as PembHutKons,'
      '(SELECT COUNT(PiutangID) FROM ARTrPiutangDt) as PembPiutang,'
      '(SELECT 0) as PembPiutKons,'
      '(SELECT 0) as PembPiuService')
