  object qu003: TADOQuery
    Connection = dmMain.dbConn
    DataSource = ds001
    Parameters = <>
    SQL.Strings = (
      
        'Select distinct Convert(varchar(10),TransDate,103)as Tanggal,Tra' +
        'nsDate,'
      '  case when FgTrans < 50 Then '#39'1'#39' else '#39'2'#39' end as Kode,         '
      
        'case Fgtrans when 0  then  '#39'Tr Stok Awal Posistif & VoucherNo '#39'+' +
        ' A.VoucherNo                               when 1  then  '#39'Tr Sto' +
        'k Opname selisih Positif & VoucherNo '#39'+ A.VoucherNo             ' +
        '                  when 2  then  '#39'Tr Tranfer Barang Ke & VoucherN' +
        'o '#39'+ A.VoucherNo                               when 3  then  '#39'Tr' +
        ' Adjustment bertambah & VoucherNo '#39'+ A.VoucherNo                ' +
        '               when 4  then  '#39'Tr PB & VoucherNo '#39'+ A.VoucherNo  ' +
        '                             when 5  then  '#39'Tr DO Retur & Vouche' +
        'rNo '#39'+ A.VoucherNo                               when 50 then  '#39 +
        'Tr Stok Awal Negatif & VoucherNo '#39'+ A.VoucherNo                 ' +
        '              when 51 then  '#39'Tr Stok Opname selisih Negatif & Vo' +
        'ucherNo '#39'+ A.VoucherNo                               when 52 the' +
        'n  '#39'Tr Tranfer Barang Dari '#39'+ A.VoucherNo                       ' +
        '        when 53 then  '#39'Tr Adjustment berkurang & VoucherNo '#39'+ A.' +
        'VoucherNo                               when 54 then  '#39'Tr DO  & ' +
        'VoucherNo '#39'+ A.VoucherNo                               when 71 t' +
        'hen  '#39'Tr Adjustment barang rusak  & VoucherNo '#39'+ A.VoucherNo    ' +
        '                           when 73 then  '#39'Tr Retur Pembelian  & ' +
        'VoucherNo '#39'+ A.VoucherNo end as Flag  ,QTy,FgTrans,'
      
        'case when A.currid='#39'IDR'#39' then a.price else a.price*a.rate end as' +
        ' Nilai'
      
        ' from Allitem A where ItemId='#39'A001'#39' AND WareHouseId='#39'GUDGUS'#39' AND' +
        ' CONVERT(varchar(8),TransDate,112) BETWEEN '#39'20060614'#39' AND '#39'20060' +
        '614'#39
      ''
      '')
    Left = 316
    Top = 92
  end
