  inherited quMain: TADOQuery
    BeforeDelete = quMainBeforeDelete
    OnCalcFields = quMainCalcFields
    OnNewRecord = quMainNewRecord
    SQL.Strings = (
      'select *, CASE Garantee WHEN '#39'1'#39' THEN '#39'Ya'#39' '
      
        '                                        WHEN '#39'0'#39' THEN '#39'Non'#39' END ' +
        'AS Garansi '
      ' from INMsItem')
