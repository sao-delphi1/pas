  object quTotal: TADOQuery
    Connection = dmMain.dbConn
    DataSource = dsMain
    Parameters = <
      item
        Name = 'PurchaseId'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 50
        Value = Null
      end
      item
        Name = 'SuppId'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 20
        Value = Null
      end>
    SQL.Strings = (
      
        'SELECT ROUND(K.SubTotal,2) as SubTotal,ROUND(CASE WHEN FgTax='#39'Y'#39 +
        ' THEN K.SubTotal*K.Rate*0.01 ELSE 0 END,2) as PPN,'
      
        'ROUND(ROUND(K.SubTotal,2)+ROUND((CASE WHEN FgTax='#39'Y'#39' THEN K.SubT' +
        'otal*K.Rate*0.01 ELSE 0 END),2),2) as GT FROM ('
      
        'SELECT A.PurchaseID,A.SuppID,B.FgTax,ISNULL(B.RatePPN,0) as Rate' +
        ',ISNULL(SUM(Price*Qty)-SUM(Price*Qty*Disc*0.01),0) as SubTotal'
      
        'FROM APTrPurchaseDt A INNER JOIN APTrPurchaseHd B on A.PurchaseI' +
        'D=B.PurchaseID'
      'GROUP BY A.PurchaseID,A.SuppID,B.FgTax,B.RatePPN) as K '
      'WHERE K.PurchaseId=:PurchaseID AND K.SuppId=:SuppId')
    Left = 768
    Top = 372
    object quTotalSubTotal: TFloatField
      FieldName = 'SubTotal'
      DisplayFormat = '#,0.00'
    end
    object quTotalPPN: TFloatField
      FieldName = 'PPN'
      ReadOnly = True
      DisplayFormat = '#,0.00'
    end
    object quTotalGT: TFloatField
      FieldName = 'GT'
      ReadOnly = True
      DisplayFormat = '#,0.00'
    end
  end
  object dsTotal: TDataSource
    DataSet = quTotal
    OnStateChange = dsDetilStateChange
    Left = 796
    Top = 372
  end
