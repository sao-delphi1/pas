      end>
    SQL.Strings = (
      'SELECT ISNULL(SUM(A.Qty*(A.Price+A.Bagasi)),0) as SubTotal,'
      
        'CASE WHEN B.FgTax='#39'T'#39' THEN 0 ELSE ISNULL(SUM(A.Qty*(A.Bagasi+A.P' +
        'rice))*B.PPN/100,0) END as PPN,'
      
        'ISNULL(CASE WHEN B.FgTax='#39'T'#39' THEN SUM(A.Qty*(A.Bagasi+A.Price)) ' +
        'ELSE SUM(Qty*(Bagasi+Price))*(1+(B.PPN/100)) END,0) as Total,'
      
        'ISNULL(SUM(A.Qty*(A.Price-A.Modal)),0) as Margin,ISNULL(SUM(A.Qt' +
        'y*A.Bagasi),0) as Bagasi,'
      
        'CASE WHEN SUM(A.Qty*A.Modal) = 0 THEN 100 ELSE (SUM(A.Qty*(A.Pri' +
        'ce-A.Modal))/SUM(A.Qty*A.Modal)*100) END as PMargin'
      'FROM ARTrPurchaseOrderDt A'
      'INNER JOIN ARTrPurchaseOrderHd B ON A.POID=B.POID'
      'WHERE A.POID=:POID'
      'GROUP BY B.FgTax,B.PPN')
