    BeforeEdit = quMainBeforeEdit
    BeforeDelete = quMainBeforeDelete
    OnCalcFields = quMainCalcFields
    OnNewRecord = quMainNewRecord
    SQL.Strings = (
      'select *,'
      'UPPER(case when jenis='#39'D'#39' then '#39'Over Due'#39
      '         when jenis='#39'L'#39' then '#39'Over Limit'#39
      '         when jenis='#39'T'#39' then '#39'Belum Proses'#39
      '         when jenis='#39'R'#39' then '#39'Jual Rugi'#39
      '         when jenis='#39'RD'#39' then '#39'Jual Rugi & Overdue'#39
      '         when jenis='#39'RL'#39' then '#39'Jual Rugi & Overlimit'#39
      '         when jenis='#39'DL'#39' then '#39'Overdue & Overlimit'#39
      '         when jenis='#39'RDL'#39' then '#39'Jual Rugi, Overdue, & Overlimit'#39
      '         when jenis='#39'Y'#39' then '#39'Approved'#39
      '         when jenis='#39'X'#39' then '#39'Rejected'#39
      'END) as Status'
      ' from artrpurchaseorderhd A')
