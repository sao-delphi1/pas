  inherited quAct: TADOQuery
    OnCalcFields = quActCalcFields
    SQL.Strings = (
      
        'select B.SaleID,B.Transdate,B.CustID,H.CustName,B.SalesID,I.Sale' +
        'sName,B.SOID,A.ItemID,C.ItemName,A.SNID,'
      'A.Price,A.Modal,'
      
        'ISNULL(D.POID,'#39#39#39#39') as POID,ISNULL(CONVERT(VARCHAR(10),G.Transda' +
        'te,111),'#39#39#39#39') as TglPO,G.SOID as SO_Beli,ISNULL(A.PurchaseID,'#39#39#39 +
        #39') as GRNID,'
      
        'ISNULL(CONVERT(VARCHAR(10),D.Transdate,111),'#39#39#39#39') as TglGRN,ISNU' +
        'LL(F.PurchaseID,'#39#39#39#39') as Invoice,ISNULL(CONVERT(VARCHAR(10),F.Tr' +
        'ansdate,111),'#39#39#39#39') as TglInv,'
      'isnull(E.Price,0) as ModalBeli,'
      
        'J.Komisi,ISNULL(A.Price+J.Komisi,0) as Titipan,B.PPNFee,ISNULL(A' +
        '.Price+J.Komisi,0)*B.PPNFee*0.01 as PriceInc,'
      
        'F.FgTax,F.Rate,isnull(E.Price*(1+(F.Rate*0.01)),0) as ModalInc,F' +
        '.FPSID'
      'from ARTrPenjualanHd B'
      'inner join ARMsCustomer H on B.CustID=H.CustID'
      'inner join ARMsSales I on B.SalesID=I.SalesID'
      'left join ARTrPenjualanSN A on A.SaleID=B.SaleID'
      'left join INMsItem C on A.ItemID=C.ItemID'
      'left join APTrKonsinyasiHd D on A.PurchaseID=D.KonsinyasiID'
      'left join APTrPurchaseDtSN E on A.SNID=E.SNID'
      'left join APTrPurchaseHd F on E.PurchaseID=F.PurchaseID'
      'left join ARTrPenawaranHd G on D.POID=G.GBUID'
      
        'left join ARTrPenjualanDt J on A.itemid=J.ItemID AND B.SaleID=J.' +
        'SaleID')
